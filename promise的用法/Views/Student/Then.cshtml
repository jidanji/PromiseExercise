
@{
    ViewBag.Title = "ThenThen";
}

<h2>ThenThen</h2>

<script>
    function p1() {
     return    new Promise(function (resolve, reject) {

            resolve({
                data: [1, 2, 3, 4],
                resposneCode: "00001",
                resposneRemark: "成功"
            })


        }
        );

    };



    function p2() { return p1().then(({ data })=> data) }

    async function GetData()
    {
        var data = await p2();
        console.log(data);
    }

    GetData();


    



</script>

